ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
ENABLE_BITCODE = YES
ENABLE_TESTABILITY = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}"/** "${PODS_CONFIGURATION_BUILD_DIR}/StoriesPlugin" "${PODS_ROOT}/ZappPlugins/Frameworks/iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}"/**
OTHER_CFLAGS = $(inherited) -fembed-bitcode -iquote "${PODS_CONFIGURATION_BUILD_DIR}/StoriesPlugin/StoriesPlugin.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AVFoundation" -framework "StoriesPlugin" -framework "ZappPlugins"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
