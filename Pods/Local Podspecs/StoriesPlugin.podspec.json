    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "swift_version": "4.2",
  "resources": [

  ],
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "YES",
    "OTHER_LDFLAGS": "$(inherited)",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}\"/**",
    "LIBRARY_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}\"/**",
    "SWIFT_VERSION": "4.1"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "frameworks": "UIKit",
      "source_files": "PluginClasses/*.{swift,h,m}",
      "dependencies": {
        "ZappPlugins": [

        ]
      }
    }
  ]
}
