{
  "name": "StoriesPlugin",
  "version": "0.0.1",
  "summary": "An Example of full screen plugin for Zapp iOS.",
  "description": "An Example of full screen plugin for Zapp iOS.",
  "homepage": "https://github.com/applicaster-plugins/Stories-iOS",
  "license": "MIT",
  "authors": {
    "Javier Casaudoumecq": "j.casaudoumecq@applicaster.com"
  },
  "source": {
